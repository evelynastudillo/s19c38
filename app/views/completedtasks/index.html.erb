<h1>Tus To-dos </h1>
<br>
<div class="container">
  <% @completedtasks.each do |completedtask| %>

  <br>

  <h3 style="background-color:#6E6E6E;"><%=  completedtask.todo.name%></h3>
  <p><%= image_tag completedtask.todo.photo %></p>
  <%= link_to 'No lo hice!',destroy_completedtask_path(todos),
  data: {confirm: 'Are you sure?'},
  method: :delete, class: 'btn btn-danger' %>

  <% end %>
</div>
